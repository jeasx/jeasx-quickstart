{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "public",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.js"
    }
  ],
  "functions": {
    "api/vercel.js": {
      "includeFiles": "{node_modules,dist,public}/**/*"
    }
  }
}
